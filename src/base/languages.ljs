	var p = premake;
	p.languages = {};

	function p.languages.isc(value) {
		return value == "C";
	}

	function p.languages.iscpp(value) {
		return value == "C++";
	}

	function p.languages.iscsharp(value) {
		return value == "C#";
	}

	function p.languages.isfsharp(value) {
		return value == "F#";
	}
